<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%- include('partials/header',{ title: univerziteti }); %>
    <table border="1" width="100%">
        <tr>
            <th>univezitet</th>
            <th>adresa</th>
            <th>fakulteti</th>
        
        </tr>
        <% data.forEach((univerzitet, i) => { %>
            <tr>
                <td> <%= univerzitet %> </td>
                <td> <%= adresa %> </td>
                <td> <%= fakulteti %> </td>
                <td>
                    <a href="/brishi?univezitetIndex=<%= i %>">Бриши</a>
                </td>
            </tr>
        <% }) %>
    </table>
</body>
</html>